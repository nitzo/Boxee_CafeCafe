<?xml version="1.0"?>
<window type="dialog" id="14010">
	
<!--**********  Window Properties **********-->

	<backgroundcolor>0</backgroundcolor>
	<allowoverlay>yes</allowoverlay>



<!--********** Controls **********-->
	   
	<controls>
		<!-- Button Control For capturing events -->
		<control type="button" id="200">
		   <description>button control for up down left and right events</description>
			<posx>80</posx>
			<posy>60</posy>
			<width>200</width>
			<height>40</height>
			<visible>true</visible>
			<colordiffuse>FFFFFFFF</colordiffuse>
			<label>Click Me!</label>
			<font>font21</font>
			<textcolor>white</textcolor>
			<disabledcolor>88FFFFFF</disabledcolor>
			<align>left</align>
			<aligny>center</aligny>
			<textoffsetx>20</textoffsetx>
			<textoffsety>-</textoffsety>
			<onup>200</onup>
			<ondown>200</ondown>		
			<onprev>200</onprev>
		</control>
	
		<!-- Full Screen Image Control -->		
		<control type="image" id="120">
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<texture>FullScreenAd.png</texture>
			<fadetime>600</fadetime>
		</control>
			
   </controls>
</window>
